<Lems>

    <!-- Specify which component to run -->
    <Target component="sim1"/>

    <!-- Include core NeuroML2 ComponentType definitions -->
    <Include file="Cells.xml"/>
    <Include file="Networks.xml"/>
    <Include file="Simulation.xml"/>

    <Include file="HCN1.channel.nml"/>
    <Include file="vClamps.xml"/>

    <vClampedCell id="holderCell_min100" delay="25ms" duration="150ms" baseVoltage="-70mV" targetVoltage="-100mV">
        <channelPopulation id="test" ionChannel="Channelpedia_HCN1_9" number="1" erev="80mV"/>
    </vClampedCell>
    <vClampedCell id="holderCell_min80" delay="25ms" duration="150ms" baseVoltage="-70mV" targetVoltage="-80mV">
        <channelPopulation id="test" ionChannel="Channelpedia_HCN1_9" number="1" erev="80mV"/>
    </vClampedCell>
    <vClampedCell id="holderCell_min60" delay="25ms" duration="150ms" baseVoltage="-70mV" targetVoltage="-60mV">
        <channelPopulation id="test" ionChannel="Channelpedia_HCN1_9" number="1" erev="80mV"/>
    </vClampedCell>
    <vClampedCell id="holderCell_min40" delay="25ms" duration="150ms" baseVoltage="-70mV" targetVoltage="-40mV">
        <channelPopulation id="test" ionChannel="Channelpedia_HCN1_9" number="1" erev="80mV"/>
    </vClampedCell>
    <vClampedCell id="holderCell_min20" delay="25ms" duration="150ms" baseVoltage="-70mV" targetVoltage="-20mV">
        <channelPopulation id="test" ionChannel="Channelpedia_HCN1_9" number="1" erev="80mV"/>
    </vClampedCell>
    <vClampedCell id="holderCell_0" delay="25ms" duration="150ms" baseVoltage="-70mV" targetVoltage="0mV">
        <channelPopulation id="test" ionChannel="Channelpedia_HCN1_9" number="1" erev="80mV"/>
    </vClampedCell>
    <vClampedCell id="holderCell_20" delay="25ms" duration="150ms" baseVoltage="-70mV" targetVoltage="20mV">
        <channelPopulation id="test" ionChannel="Channelpedia_HCN1_9" number="1" erev="80mV"/>
    </vClampedCell>
    
    <vClampedRampCell id="rampCell0" delay="0ms" duration="200ms" baseVoltage="-100mV" targetVoltage0="-100mV" targetVoltage1="100mV">
        <channelPopulation id="test" ionChannel="Channelpedia_HCN1_9" number="1" erev="80mV"/>
    </vClampedRampCell>

    
    <network id="net1">
        
        <population id="holderCellPop_min100" component="holderCell_min100" size="1"/>
        <population id="holderCellPop_min80" component="holderCell_min80" size="1"/>
        <population id="holderCellPop_min60" component="holderCell_min60" size="1"/>
        <population id="holderCellPop_min40" component="holderCell_min40" size="1"/>
        <population id="holderCellPop_min20" component="holderCell_min20" size="1"/>
        <population id="holderCellPop_0" component="holderCell_0" size="1"/>
        <population id="holderCellPop_20" component="holderCell_20" size="1"/>
        
        <population id="rampCellPop0" component="rampCell0" size="1"/>
    </network>


    <Simulation id="sim1" length="200ms" step="0.05ms" target="net1">
        
        <Display id="d0" title="Channelpedia_HCN1_9: Clamp voltages"  timeScale="1ms" xmin="-20.0" xmax="220.0" ymin="-120" ymax="40">
            <Line id="-100mV" quantity="holderCellPop_min100[0]/v" scale="1mV"  color="#ffff00" timeScale="1ms"/>
            <Line id="-80mV" quantity="holderCellPop_min80[0]/v" scale="1mV"  color="#ffd400" timeScale="1ms"/>
            <Line id="-60mV" quantity="holderCellPop_min60[0]/v" scale="1mV"  color="#ffaa00" timeScale="1ms"/>
            <Line id="-40mV" quantity="holderCellPop_min40[0]/v" scale="1mV"  color="#ff7f00" timeScale="1ms"/>
            <Line id="-20mV" quantity="holderCellPop_min20[0]/v" scale="1mV"  color="#ff5500" timeScale="1ms"/>
            <Line id="0mV" quantity="holderCellPop_0[0]/v" scale="1mV"  color="#ff2a00" timeScale="1ms"/>
            <Line id="20mV" quantity="holderCellPop_20[0]/v" scale="1mV"  color="#ff0000" timeScale="1ms"/>
        </Display>

        <Display id="d1" title="Channelpedia_HCN1_9: Fractional conductance"  timeScale="1ms" xmin="-20.0" xmax="220.0" ymin="-0.1" ymax="1.1">
            <Line id="-100mV" quantity="holderCellPop_min100[0]/test/Channelpedia_HCN1_9/fopen" scale="1"  color="#ffff00" timeScale="1ms"/>
            <Line id="-80mV" quantity="holderCellPop_min80[0]/test/Channelpedia_HCN1_9/fopen" scale="1"  color="#ffd400" timeScale="1ms"/>
            <Line id="-60mV" quantity="holderCellPop_min60[0]/test/Channelpedia_HCN1_9/fopen" scale="1"  color="#ffaa00" timeScale="1ms"/>
            <Line id="-40mV" quantity="holderCellPop_min40[0]/test/Channelpedia_HCN1_9/fopen" scale="1"  color="#ff7f00" timeScale="1ms"/>
            <Line id="-20mV" quantity="holderCellPop_min20[0]/test/Channelpedia_HCN1_9/fopen" scale="1"  color="#ff5500" timeScale="1ms"/>
            <Line id="0mV" quantity="holderCellPop_0[0]/test/Channelpedia_HCN1_9/fopen" scale="1"  color="#ff2a00" timeScale="1ms"/>
            <Line id="20mV" quantity="holderCellPop_20[0]/test/Channelpedia_HCN1_9/fopen" scale="1"  color="#ff0000" timeScale="1ms"/>
        </Display>
        
        <Display id="d7" title="Channelpedia_HCN1_9: Currents"  timeScale="1ms" xmin="-20.0" xmax="220.0" ymin="-0.00002" ymax="0.0014">
            <Line id="-100mV" quantity="holderCellPop_min100[0]/test/i" scale="1nA"  color="#ffff00" timeScale="1ms"/>
            <Line id="-80mV" quantity="holderCellPop_min80[0]/test/i" scale="1nA"  color="#ffd400" timeScale="1ms"/>
            <Line id="-60mV" quantity="holderCellPop_min60[0]/test/i" scale="1nA"  color="#ffaa00" timeScale="1ms"/>
            <Line id="-40mV" quantity="holderCellPop_min40[0]/test/i" scale="1nA"  color="#ff7f00" timeScale="1ms"/>
            <Line id="-20mV" quantity="holderCellPop_min20[0]/test/i" scale="1nA"  color="#ff5500" timeScale="1ms"/>
            <Line id="0mV" quantity="holderCellPop_0[0]/test/i" scale="1nA"  color="#ff2a00" timeScale="1ms"/>
            <Line id="20mV" quantity="holderCellPop_20[0]/test/i" scale="1nA"  color="#ff0000" timeScale="1ms"/>
        </Display>

        <Display id="m_inf" title="Channelpedia_HCN1_9: Gate m steady state, m_inf"  timeScale="1ms" xmin="-20.0" xmax="220.0" ymin="-0.1" ymax="1.1">
            <Line id="m_inf" quantity="rampCellPop0[0]/test/Channelpedia_HCN1_9/m/inf" scale="1"  color="#ff0000" timeScale="1ms"/>
        </Display>
        
        <Display id="m_tau" title="Channelpedia_HCN1_9: Gate m time course, m_tau"  timeScale="1ms" xmin="-20.0" xmax="220.0" ymin="-10" ymax="110">
            <Line id="m_tau" quantity="rampCellPop0[0]/test/Channelpedia_HCN1_9/m/tau" scale="1ms"  color="#ff0000" timeScale="1ms"/>
        </Display>


    </Simulation>


</Lems>
